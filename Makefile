# This file is for those not familiar with Java
# Use ./gradlew instead of make!

info:
	./gradlew info
	
clean:
	./gradlew clean

all:
	./gradlew all
	
current:
	./gradlew current
	
linux:
	./gradlew linux
	
linux64:
	./gradlew linux64
	
win32:
	./gradlew win32
	
win64:
	./gradlew win64
	
mac:
	./gradlew mac
	
